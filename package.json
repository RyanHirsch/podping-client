{
  "name": "podping-client",
  "version": "1.0.2",
  "main": "dist/index.js",
  "author": "Ryan Hirsch <ryan.hirsch@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/bytebuffer": "^5.0.42",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@types/pino": "^6.3.11",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "esbuild": "^0.12.25",
    "esbuild-runner": "^2.2.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "prettier:base": "prettier --parser typescript",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "start": "node dist/index.js",
    "prebuild": "rimraf dist",
    "build": "npm-run-all -p build:*",
    "build:src": "esbuild src/index.ts  --bundle --platform=node --target=node14 --outfile=dist/index.js",
    "build:types": "tsc --project tsconfig.types.json",
    "test": "jest",
    "lint": "tsc --noEmit && eslint --ext .js,.ts src/",
    "test:watch": "jest --watch",
    "dev": "esr src/index.ts",
    "dev:watch": "ts-node-dev --respawn --no-notify src/index.ts"
  },
  "dependencies": {
    "@hiveio/dhive": "^1.0.1",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "pino": "^6.13.2",
    "rxjs": "^7.3.0"
  }
}
